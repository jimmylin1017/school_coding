文法

    0 $accept: START $end

    1 class_modifier: PROTECTED
    2               | PRIVATE
    3               | PUBLIC

    4 data_modifier: STATIC
    5              | FINAL
    6              | CONST

    7 data_type: VOID
    8          | INT
    9          | FLOAT
   10          | DOUBEL
   11          | CHAR
   12          | BOOLEAN
   13          | STRING
   14          | EXCEPTION

   15 prefix: DOUBLE_OPERATOR
   16       | '+'
   17       | '-'

   18 postfix: DOUBLE_OPERATOR

   19 const_expr: REAL_DATA
   20           | INT_DATA
   21           | STRING_DATA

   22 START: %empty
   23      | class_declarations START

   24 class_declarations: class_modifier CLASS ID '{' statement '}'
   25                   | CLASS ID '{' statement '}'
   26                   | class_modifier CLASS ID class_extends '{' statement '}'
   27                   | CLASS ID class_extends '{' statement '}'
   28                   | class_modifier CLASS ID class_implements class_implements_list '{' statement '}'
   29                   | CLASS ID class_implements class_implements_list '{' statement '}'

   30 class_extends: EXTENDS ID

   31 class_implements: IMPLEMENTS ID

   32 class_implements_list: %empty
   33                      | ',' ID class_implements_list

   34 variable_declarations: data_modifier data_type identifier_declarations ';'
   35                      | data_type identifier_declarations ';'
   36                      | ID identifier_declarations ID '(' ')' ';'
   37                      | error

   38 identifier_declarations: identifier identifier_list
   39                        | '[' ']' array array_list

   40 identifier: ID
   41           | ID EQUAL expression
   42           | ID EQUAL NEW

   43 identifier_list: %empty
   44                | ',' identifier identifier_list

   45 array: ID EQUAL NEW data_type '[' expression ']'

   46 array_list: %empty
   47           | ',' array array_list

   48 condition: IF '(' boolean_expr boolean_expr_list ')' simple_or_compound
   49          | IF '(' boolean_expr boolean_expr_list ')' simple_or_compound ELSE simple_or_compound

   50 simple_or_compound: simple
   51                   | compound

   52 boolean_expr: expression COMPARE_BOOL expression
   53             | TRUE
   54             | FALSE

   55 boolean_expr_list: %empty
   56                  | CONNECT_BOOL boolean_expr boolean_expr_list

   57 compound: '{' statement '}'

   58 loop: WHILE '(' boolean_expr boolean_expr_list ')' loop_simple_or_compound
   59     | DO loop_compound WHILE '(' boolean_expr boolean_expr_list ')' ';'
   60     | FOR '(' for_init ';' boolean_expr boolean_expr_list ';' for_update ')' loop_simple_or_compound

   61 loop_condition: IF '(' boolean_expr boolean_expr_list ')' loop_simple_or_compound
   62               | IF '(' boolean_expr boolean_expr_list ')' loop_simple_or_compound ELSE loop_simple_or_compound

   63 loop_simple_or_compound: loop_simple
   64                        | loop_compound

   65 for_init: data_type for_init_assign for_init_assign_list
   66         | for_init_assign for_init_assign_list

   67 for_init_assign: ID EQUAL expression

   68 for_init_assign_list: %empty
   69                     | ',' for_init_assign for_init_assign_list

   70 for_update: ID postfix
   71           | prefix ID

   72 loop_compound: '{' loop_statement '}'

   73 try_catch_finally: TRY compound CATCH '(' parameter ')' compound FINALLY compound
   74                  | TRY compound CATCH '(' parameter ')' compound

   75 switch_case_default: SWITCH '(' ID ')' switch_compound

   76 switch_compound: '{' switch_statement '}'
   77                | '{' switch_statement DEFAULT ':' statement '}'

   78 switch_statement: %empty
   79                 | CASE const_expr ':' statement BREAK ';' switch_statement
   80                 | CASE const_expr ':' statement switch_statement

   81 parameter: %empty
   82          | argv argv_list

   83 argv: data_type ID

   84 argv_list: %empty
   85          | ',' argv argv_list

   86 statement: %empty
   87          | compound statement
   88          | simple statement
   89          | condition statement
   90          | loop statement
   91          | return statement
   92          | method_declaration statement
   93          | variable_declarations statement
   94          | try_catch_finally statement
   95          | switch_case_default statement

   96 loop_statement: %empty
   97               | loop_compound loop_statement
   98               | simple loop_statement
   99               | loop_condition loop_statement
  100               | loop loop_statement
  101               | return loop_statement
  102               | method_declaration loop_statement
  103               | variable_declarations loop_statement
  104               | try_catch_finally loop_statement
  105               | CONTINUE ';'
  106               | BREAK ';'

  107 method_declaration: class_modifier data_modifier data_type ID '(' parameter ')' '{' statement '}'
  108                   | class_modifier data_type ID '(' parameter ')' '{' statement '}'
  109                   | data_type ID '(' parameter ')' '{' statement '}'
  110                   | MAIN '(' ')' '{' statement '}'

  111 loop_simple: name EQUAL expression ';'
  112            | PRINT '(' expression ')' ';'
  113            | READ '(' name ')' ';'
  114            | expression ';'
  115            | CONTINUE ';'
  116            | BREAK ';'

  117 simple: name EQUAL expression ';'
  118       | PRINT '(' expression ')' ';'
  119       | READ '(' name ')' ';'
  120       | expression ';'

  121 name: ID
  122     | ID '.' ID
  123     | THIS '.' ID

  124 expression: term
  125           | expression '+' term
  126           | expression '-' term

  127 term: factor factor_list

  128 factor: ID
  129       | ID '[' ID ']'
  130       | ID '[' INT_DATA ']'
  131       | const_expr
  132       | '(' expression ')'
  133       | prefix name
  134       | name postfix
  135       | method_invocation

  136 factor_list: %empty
  137            | '*' factor
  138            | '/' factor

  139 method_invocation: name '(' argument ')'

  140 argument: %empty
  141         | expression argument_list

  142 argument_list: %empty
  143              | ',' expression argument_list

  144 return: RETURN expression ';' '\n'


